{
    "makedir" : "MAKE/",
    "PDMO" : "../../",
    "incdir" : "inc/",
    "srctypes" : {
        "asm" : ["asm","s"],
        "cpp" : ["cpp","cxx"],
        "c" : ["c"]
    },
    "util" : {
        "cc" : "gcc-12",
        "cxx" : "g++-12",
        "as" : "gcc-12",
        "ld" : "ld"
    },
    "include" : {
        "kernel" : ["modules/klib/inc/"]
    },
    "args" : {
        "kernelNoWarn" : {
            "as" : [],
            "c++" : ["-Wno-literal-suffix"],
            "c" : [],
            "ld" : []
        },
        "kernel" : {
            "as" : [],
            "c++" : ["-O0","-nostdlib","-ffreestanding","-fno-stack-protector"],
            "c" : [],
            "ld" : []
        },
        "dependency" : {
            "as" : ["-M","-MD"],
            "c++" : ["-MP","-MD"],
            "c" : [],
            "ld" : []
        }
    },
    "kmods" : {
        "INIT" : {
            "output" : "out/kernel/INIT.elf64",
            "opath" : "build/INIT/",
            "spath" : "modules/INIT/",
            "lds" : "LDS/INIT.ld",
            "inc" : ["kernel"],
            "args" : ["kernel","kernelNoWarn"]
        },
        "FUNCDIS" : {
            "output" : "out/kernel/FUNCDIS.elf64",
            "opath" : "build/FUNCDIS/",
            "spath" : "modules/FUNCDIS/",
            "lds" : "LDS/FUNCDIS.ld",
            "inc" : ["kernel"],
            "args" : ["kernel","kernelNoWarn"]
        },
        "klib" : {
            "output" : "out/klib.elf64",
            "opath" : "build/klib/",
            "spath" : "modules/klib/",
            "lds" : "LDS/klib.ld",
            "inc" : [],
            "args" : []
        },
        "MALLOC" : {
            "output" : "out/MALLOC.elf64",
            "opath" : "build/MALLOC/",
            "spath" : "modules/MALLOC/",
            "lds" : "LDS/MALLOC.ld",
            "inc" : [],
            "args" : []
        }
    }
}