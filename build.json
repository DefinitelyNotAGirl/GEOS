{
    "makedir" : "MAKE/",
    "PDMO" : "../../",
    "incdir" : "inc/",
    "srctypes" : {
        "asm" : ["asm","s"],
        "cpp" : ["cpp","cxx"],
        "c" : ["c"]
    },
    "util" : {
        "cc" : "x86_64-elf-gcc",
        "cxx" : "x86_64-elf-g++",
        "as" : "x86_64-elf-as",
        "ld" : "x86_64-elf-ld"
    },
    "include" : {
        "kernel" : ["modules/klib/inc/","inc/"]
    },
    "args" : {
        "kernelNoWarn" : {
            "as" : [],
            "c++" : ["-Wno-literal-suffix"],
            "c" : [],
            "ld" : []
        },
        "kernel" : {
            "as" : [],
            "c++" : ["-O0","-nostdlib","-ffreestanding","-fno-stack-protector"],
            "c" : [],
            "ld" : []
        },
        "dependency" : {
            "as" : ["-M","-MD"],
            "c++" : ["-MP","-MD"],
            "c" : [],
            "ld" : []
        },
        "system" : {
            "aarch64-OSX" : {
                "as" : [""],
                "c++" : ["-march=x86-64"],
                "c" : ["-march=x86-64"],
                "ld" : [""]
            },
            "x86_64-gnu_linux" : {
                "as" : [""],
                "c++" : ["-march=x86-64"],
                "c" : ["-march=x86-64"],
                "ld" : [""]
            }
        }
    },
    "kmods" : {
        "INIT" : {
            "output" : "out/kernel/INIT.elf64",
            "opath" : "build/INIT/",
            "spath" : "modules/INIT/",
            "lds" : "LDS/INIT.ld",
            "inc" : ["kernel"],
            "args" : ["kernel","kernelNoWarn","dependency"]
        },
        "FUNCDIS" : {
            "output" : "out/kernel/FUNCDIS.elf64",
            "opath" : "build/FUNCDIS/",
            "spath" : "modules/FUNCDIS/",
            "lds" : "LDS/FUNCDIS.ld",
            "inc" : ["kernel"],
            "args" : ["kernel","kernelNoWarn","dependency"]
        },
        "klib" : {
            "output" : "out/kernel/klib.elf64",
            "opath" : "build/klib/",
            "spath" : "modules/klib/",
            "lds" : "LDS/klib.ld",
            "inc" : ["kernel"],
            "args" : ["kernel","kernelNoWarn","dependency"]
        },
        "MALLOC" : {
            "output" : "out/kernel/MALLOC.elf64",
            "opath" : "build/MALLOC/",
            "spath" : "modules/MALLOC/",
            "lds" : "LDS/MALLOC.ld",
            "inc" : ["kernel"],
            "args" : ["kernel","kernelNoWarn","dependency"]
        }
    }
}